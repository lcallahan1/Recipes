    <div id="signin-widget-content" class="widget-content"><div id="login-tab" class="notLoggedIn"><div id="login-tab-inner"><a onclick="return mtSignInOnClick('login-tab-inner')" href="javascript:void(0)" id="login-link">log in</a></div></div><div class="clearer"><!-- --></div></div>
