                        $(document).ready(function() {
                        	   var fullAddPrivateTxt 	= $("#textnoteformore").text();
                        	   var fullUserNoteTxt 		= $("#usertextnote").text();
                        	  	$('.morelinkfornote').live('hover', function() {
                        			$('.textnotemore').html(fullAddPrivateTxt);
                        		});
                        		$('.morelinkforusernote').live('hover', function() {
                        			$('.userclsnote').html(fullUserNoteTxt);
                        		});
                        		$('.my-pn').mouseleave(function(){
                        			resizeAddPrivateNote();
                        	   });
                        	   $('.userNoteCont').mouseleave(function(){
                        			resizeUserNote();
                  	   		    });
                 	   		    
                        	   $('.noteDel').click(function() {
                       			$('.MyDeleteNote').attr('style', 'display:block');
                       		   });

                        	   $('.pnote-cancel').click(function() {
                          			$('.MyDeleteNote').attr('style', 'display:none');
                          	    });

                        	   function resizeAddPrivateNote() {
                        	        var fullTxtLength = fullAddPrivateTxt.length;
                        	        if(fullTxtLength >= 250){
                        			var $shortMessage = fullAddPrivateTxt.substr(0,250)+ ' ...';
                        			$('.textnotemore').html($shortMessage);
                        			$('.textnotemore').append('<div class="morelinkfornote"><a href="javascript:void(0);">More</a></div>');
                        			}
                        	   }

                        	   function resizeUserNote() {
                       	        var fullTxtLength = fullUserNoteTxt.length;
                       	        if(fullTxtLength >= 250){
                       				var $shortMessage = fullUserNoteTxt.substr(0,250)+ ' ...';
                       				$('.morelinkforusernote').remove();
                       				$('.userclsnote').html( $shortMessage);
                       				$('.userclsnote').append('<div class="morelinkforusernote"><a href="javascript:void(0);">More</a></div>');
                       				}
                       	     	}
                      	     	
                        		resizeAddPrivateNote();
                        		resizeUserNote();
                        	  });
                        function removeNotes()
						{
                        	var request=document.all?new ActiveXObject("Microsoft.XMLHTTP"):new XMLHttpRequest();
							var url="/dpn.do?pid=0&mid=0&rid=284057";
							request.onreadystatechange=function()
							{
								if(request.readyState==4)
								{
									if(request.status==200)
									{
										document.location = document.location+"";
									}
								}
							}
							request.open("GET", url,true);
							request.send(null);
						}
					</script>
					
